{
  "name": "yahoo-finance-mcp",
  "version": "1.1.1",
  "description": "Multi-usage Yahoo Finance library supporting MCP server and LlamaIndex tool integration",
  "main": "build/index.js",
  "type": "module",
  "exports": {
    ".": "./build/index.js",
    "./tools": "./build/tools.js",
    "./llamaindex": "./build/llamaindex-tools.js",
    "./mcp": "./build/mcp-server.js"
  },
  "bin": {
    "yahoo-finance-mcp": "./build/mcp-server.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:mcp": "tsx src/mcp-server.ts",
    "start": "node build/mcp-server.js",
    "start:mcp": "node build/mcp-server.js",
    "example": "tsx src/examples.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "yahoo-finance",
    "finance",
    "stocks"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "@llamaindex/core": "^0.6.17",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "yahoo-finance2": "^3.4.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
